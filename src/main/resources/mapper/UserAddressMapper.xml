<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yuanxueqi.exam.dao.UserAddressMapper">


  <select id="select" resultType="String">
    select address from bind where user_id=#{userId}
  </select>
  <insert id="insert" parameterType="com.yuanxueqi.exam.data.UserAddress">
    insert into bind(user_id,address,state) values(#{userId},#{address},#{state})
  </insert>
</mapper>